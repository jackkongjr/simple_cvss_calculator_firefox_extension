{

  "description": "Adds a browser action icon to the toolbar. Click the button to calculate the CVSS 3.1 Base Score for a vulnerability",
  "manifest_version": 2,
  "name": "CVSS31Calc",
  "version": "1.0",
  "homepage_url": "https://github.com/jackkongjr/simple_cvss_calculator_firefox_extension",
  "icons": {
    "48": "images/icon48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "images/icon32.png",
    "theme_icons": [{
        "light": "images/icon32.png",
        "dark": "images/icon32.png",
        "size": 32
    }],
    "default_title": "CVSS 31 Calculator",
    "default_popup": "popup/popup.html"
  },

  "web_accessible_resources": [
    
  ]

}
